{
  "name": "@imbuejs/svelte",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "nodemon --config nodemon.json -L -e svelte,ts,js,json,cjs --exec 'npm run clean && npm run package && svelte-kit dev'",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "package": "svelte-kit package",
    "clean": "rimraf ./package",
    "deriveMdnData": "ts-node scripts/derivePropertyNamesFromMDN.cjs && prettier --write src/lib/mdn/derived",
    "pre:test": "node scripts/pre-test.cjs",
    "test:cmd": "uvu -r ts-node/register -r jsdom-global/register tests",
    "post:test": "node scripts/post-test.cjs",
    "test": "nodemon --config nodemon.test.json -L -e svelte,ts,js,json,cjs --exec 'pnpm pre:test && pnpm test:cmd && pnpm post:test'",
    "test:coverage": "npm run pre:test && c8 --include=src/lib npm run test:cmd && npm run post:test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "prepublish": "npm run package && node scripts/amendPackageExports.cjs",
    "publish": "npm publish ./package"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^2.0.1",
    "@types/color": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "c8": "^7.7.3",
    "chalk": "^4.1.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "global-jsdom": "^8.1.0",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.1",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "svelte2tsx": "^0.4.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.0",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/serialize": "^1.0.2",
    "@emotion/sheet": "^1.0.1",
    "color": "^3.1.3",
    "csstree-validator": "^2.0.1",
    "stylis": "^4.0.10"
  }
}